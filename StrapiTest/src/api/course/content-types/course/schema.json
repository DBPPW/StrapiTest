{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "courseName": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "peopleLimit": {
      "type": "integer",
      "required": true,
      "default": 20
    },
    "lessonStartTime": {
      "type": "datetime",
      "required": true
    },
    "lessonEndTime": {
      "type": "datetime",
      "required": true
    },
    "applyStartTime": {
      "type": "datetime",
      "required": true
    },
    "applyEndTime": {
      "type": "datetime",
      "required": true
    },
    "applicants": {
      "type": "integer"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "course"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "courses"
    }
  }
}
